#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main () {
    
    setlocale(0, "RU");
    float a, b;
    cout << "Take number 1 ";
    cin >> a;
    cout << "Take number 2 ";
    cin >> b;
    if (a > b) {
        cout << "a is more" << endl;}
    else if (a < b) {
        cout << "b is more" << endl;}
    else {
        cout << "Numbers are equal" << endl;}
    return 0 ;
   
}
